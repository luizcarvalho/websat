<h1><%=h labels('changing') + " " + labels('registration') %></h1>

<%= error_messages_for :alias %>

<% form_for(:alias, :url => object_url, :html => { :method => :put }) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%=submit_tag labels('update') %>
  </p>
<% end %>

